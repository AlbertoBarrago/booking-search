{
  "name": "@balby/booking-search",
  "version": "1.0.9",
  "publishConfig": {
    "access": "public"
  },
  "description": "Framework-agnostic React component for booking search, inspired by Booking.com",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "style": "./dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles": {
      "import": "./dist/index.css",
      "require": "./dist/index.css",
      "default": "./dist/index.css"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlbertoBarrago/booking-search.git"
  },
  "bugs": {
    "url": "https://github.com/AlbertoBarrago/booking-search/issues"
  },
  "homepage": "https://AlbertoBarrago.github.io/booking-search/",
  "keywords": [
    "react",
    "booking",
    "search",
    "widget",
    "typescript",
    "shadcn",
    "tailwind",
    "react-component",
    "booking-search",
    "date-picker",
    "location-search"
  ],
  "author": "Alberto Barrago",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "bun run build:css && bun --watch index.ts",
    "dev:css": "bun run build:css && bunx concurrently \"bun --watch index.ts\" \"bun run watch:css\"",
    "build": "bun run build:css && bun build src/index.ts --outdir=dist --target=browser --format=esm --external react --external react-dom --external react/jsx-runtime",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css --minify",
    "build:types": "tsc -p tsconfig.build.json",
    "watch:css": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css --watch",
    "type-check": "tsc --noEmit",
    "test": "bun test src/components/booking-search/tests/",
    "test:coverage": "bun test --coverage src/components/booking-search/tests/",
    "prepublishOnly": "bun run type-check && bun run test && bun run build"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^20.0.11",
    "@testing-library/react": "^16.3.1",
    "@types/bun": "latest",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.4.23",
    "happy-dom": "^20.0.11",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwindcss": ">=3.0.0"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "react-day-picker": "8.10.1",
    "tailwind-merge": "^3.4.0"
  }
}
