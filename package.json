{
  "name": "@booking-widget/react",
  "version": "1.0.0",
  "description": "Componente React agnostico per ricerca booking, ispirato a Booking.com",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "files": [
    "src",
    "README.md"
  ],
  "keywords": [
    "react",
    "booking",
    "search",
    "widget",
    "typescript",
    "shadcn",
    "tailwind"
  ],
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "bun run build:css && bun --hot index.ts",
    "build": "bun run build:css && bun build src/index.ts --outdir=dist --target=browser --format=esm",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css --minify",
    "watch:css": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.17"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "baseline-browser-mapping": "^2.9.10",
    "caniuse-lite": "^1.0.30001760",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "react": "18.3.1",
    "react-day-picker": "8.10.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "^3.4.0"
  }
}
